          &lt;td style=&#34;text-align: center&#34;&gt;*i+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→*FT&#39;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&amp;rsquo;F&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;i+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&amp;rsquo;i&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;i+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;F→i&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&#39;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&#39;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→$\epsilon$&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T+&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;+i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;E&amp;rsquo;→+TE&#39;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&amp;rsquo;F&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T→FT&#39;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&amp;rsquo;i&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;i$&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;F→i&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$E&amp;rsquo;T&#39;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;$&lt;/td&gt;
