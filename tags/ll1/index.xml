          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&#39;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→ε&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→ε&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→*FT&#39;&lt;/td&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;T&amp;rsquo;→ε&lt;/td&gt;
      &lt;/tr&gt;
      &lt;tr&gt;
          &lt;td style=&#34;text-align: center&#34;&gt;F&lt;/td&gt;
